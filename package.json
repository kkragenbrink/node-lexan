{
  "name": "lexan",
  "version": "1.0.1",
  "description": "A lexical analyzer for nodejs.",
  "main": "index.js",
  "keywords": [
    "tokenizer",
    "token",
    "lexer",
    "parser",
    "analyzer",
    "string"
  ],
  "scripts": {
    "cover": "NODE_ENV=test ./node_modules/.bin/istanbul cover --report lcov -- node_modules/mocha/bin/_mocha --recursive test --check-leaks --sort --reporter spec",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "npm run-script cover && npm run-script lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kkragenbrink/node-lexan"
  },
  "engines": {
    "nodejs": ">= 4.0.x"
  },
  "author": "Kevin Kragenbrink <kevin@gaikai.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "extend": "^3.0.0"
  }
}
